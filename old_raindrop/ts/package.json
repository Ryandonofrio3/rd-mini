{
  "name": "raindrop-ai",
  "version": "0.0.72",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tracing": {
      "types": "./dist/tracing/index.d.ts",
      "node": "./dist/tracing/index.js",
      "import": "./dist/tracing/index.mjs",
      "require": "./dist/tracing/index.js"
    },
    "./otel": {
      "types": "./dist/otel/index.d.ts",
      "node": "./dist/otel/index.js",
      "import": "./dist/otel/index.mjs",
      "require": "./dist/otel/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo && rm -rf dist",
    "try": "tsx ./src/example/index.ts",
    "test": "vitest run",
    "smoke": "tsx ./scripts/smoke.ts"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsup": "^8.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3",
    "vitest": "^4.0.10"
  },
  "dependencies": {
    "@dawn-analytics/redact-pii": "0.1.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/resources": "^2.0.1",
    "weakref": "^0.2.1",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "@traceloop/node-server-sdk": "0.19.0-otel-v1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": false
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/tracing/index.ts",
      "src/otel/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "@traceloop/node-server-sdk"
    ],
    "noExternal": [
      "@dawn/schemas"
    ],
    "dts": {
      "resolve": true
    },
    "clean": true
  }
}
